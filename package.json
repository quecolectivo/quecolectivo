{
  "name": "quecolectivo2",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "pushstate-server": "^2.1.0",
    "react-scripts": "^0.8.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-extension": "^1.0.0",
    "standard": "^8.6.0",
    "sw-precache": "^4.2.3",
    "sw-toolbox": "^3.4.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "google-map-react": "^0.22.0",
    "leaflet": "^1.0.2",
    "material-ui": "^0.16.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-leaflet": "next",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0",
    "redux-thunk": "^2.1.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "dev:https": "HTTPS=true yarn run dev",
    "build": "react-scripts build && sw-precache --config='./sw-precache-config.js'",
    "build:serve": "yarn run build && yarn run serve",
    "serve": "pushstate-server build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "build:deploy": "yarn run build && yarn run deploy",
    "lint": "cd src && standard || exit 0",
    "lint:fix": "cd src && standard --fix || exit 0"
  },
  "homepage": "https://quecolectivo.github.io/quecolectivo",
  "eslintConfig": {
    "extends": "react-app"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
